<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.paint.LinearGradient?>
<?import javafx.scene.paint.Stop?>
<?import javafx.scene.shape.Circle?>
<?import javafx.scene.shape.Rectangle?>

<AnchorPane onMouseClicked="#paneMouseClicked" prefHeight="500.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/8.0.131" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.AnimationWindowController">
   <children>
      <HBox layoutX="24.0" layoutY="14.0" spacing="10.0" AnchorPane.leftAnchor="25.0" AnchorPane.topAnchor="25.0">
         <children>
            <Button mnemonicParsing="false" onAction="#playAnimation" text="Play" />
            <Button mnemonicParsing="false" onAction="#pauseAnimation" text="Pause" />
            <Button mnemonicParsing="false" onAction="#stopAnimation" text="Stop" />
            <Button mnemonicParsing="false" onAction="#closeAnimationWindow" text="Close" />
         </children>
      </HBox>
      <Rectangle fx:id="rect" arcHeight="5.0" arcWidth="5.0" height="100.0" layoutX="30.0" layoutY="160.0" stroke="BLACK" strokeType="INSIDE" strokeWidth="3.0" width="100.0">
         <fill>
            <LinearGradient endX="1.0" endY="1.0">
               <stops>
                  <Stop color="#1f5cff" />
                  <Stop color="#1fffd1" offset="0.5166666666666666" />
                  <Stop color="WHITE" offset="1.0" />
               </stops>
            </LinearGradient>
         </fill>
      </Rectangle>
      <Rectangle fx:id="rect1" arcHeight="5.0" arcWidth="5.0" height="100.0" layoutX="185.0" layoutY="300.0" stroke="BLACK" strokeType="INSIDE" strokeWidth="3.0" width="100.0">
         <fill>
            <LinearGradient endX="1.0" endY="1.0">
               <stops>
                  <Stop color="#1f5cff" />
                  <Stop color="#1fffd1" offset="0.5166666666666666" />
                  <Stop color="WHITE" offset="1.0" />
               </stops>
            </LinearGradient>
         </fill>
      </Rectangle>
      <Circle fx:id="circle" fill="#1f93ff00" layoutX="360.0" layoutY="192.0" radius="100.0" stroke="#0242ff" strokeType="INSIDE" strokeWidth="6.0" visible="false" />
   </children>
</AnchorPane>
